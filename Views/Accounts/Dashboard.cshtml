@model Amazon.Extensions.CognitoAuthentication.CognitoUser
@{
    ViewData["Title"] = "Dashboard";
}

<h1>Dashboard</h1>

<dl class="row">
    <dt class="col-sm-2">
        UserId
    </dt>
    <dd class="col-sm-10">
        @Model.UserID
    </dd>
</dl>
<dl class="row">
    <dt class="col-sm-2">
        Username
    </dt>
    <dd class="col-sm-10">
        @Model.Username
    </dd>
</dl>
<dl class="row">
    <dt class="col-sm-2">
        Email
    </dt>
    <dd class="col-sm-10">
      @Model.Attributes["email"]
    </dd>
</dl>
